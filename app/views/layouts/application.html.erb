<!DOCTYPE html>
<html>
<head>
  <title>Forum</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="page">
  <div id="header">
    <%= render :partial => "layouts/header" %>
  </div>
  <% flash.each do |key, value| %>
    <div class="flash <%= key %>"<%= value %></div>
  <% end %>
  <%= yield %>
  <div id="footer">
    <!-- Footer goes here -->
  </div>
</div>
</body>
</html>
